a = int(input())
b = int(input())
c =int(input())
d = int(input())
nechet = 0
chet = 0
if (a % 2 > 0):
    nechet = a + nechet
else :
    chet = a + chet
if (b % 2 > 0):
    nechet = b + nechet
else :
    chet = b + chet
if (c % 2 > 0):
    nechet = c + nechet
else :
    chet = c + chet
if (d % 2 > 0):
    nechet = d + nechet
else :
    chet = d + chet
print ('сумма не чет. ',nechet)
print ('сумма чет. ',chet)